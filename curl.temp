function isTemperatureCharEntered(pTextbox)
{
  var varVal = pTextbox.value;
  var y = 0;
  var prevStr = "";
  var s = "";
  var c = "";
  var count = 0;
  y = varVal.length;
  var pattern = /^\d{1,3}(\.(\d{1})?)?$/;

  for (x = 0; x < y; x++) {
    c = varVal.substr(x, 1);
    prevStr = s;
    s += c;
    if ((c == '\b') || (pattern.test(s))) {
