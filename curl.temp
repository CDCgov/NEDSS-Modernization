function isNumericString(pString)
{
    //  Establish a pattern:  one or more digits.
    var varPattern = /^\d+$/;
    //  Perform a regular expression match.
    var varMatch = pString.match(varPattern);
    if(varMatch == null)
    {
        //  The match failed.
        //  The string is not numeric.
        return false;
    }
    //  The match succeeded.
    //  The string is numeric.
    return true;
}


