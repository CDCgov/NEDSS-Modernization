	}
	if (fromBoolNode.value=="Y") {
		toTextBoxLabel.style.color="#000";

		enableAllBrowsers($j("#" + toTextField).parent().parent().find(":input"));
		$j("#" + toTextField).parent().parent().find("img").attr("disabled", false);
