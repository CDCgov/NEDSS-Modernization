type Query {
    findPatientById(id: ID!): Person
    findAllPatients(page: Page): [Person]
    findPatientsByFilter(filter: PersonFilter!): [Person]

    findOrganizationById(id: ID!): Organization
    findAllOrganizations(page: Page): [Organization]
    findOrganizationsByFilter(filter: OrganizationFilter!): [Organization]
}
