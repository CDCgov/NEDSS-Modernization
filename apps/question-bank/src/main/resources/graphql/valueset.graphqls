type Mutation {
  createValueSet(data: ValueSetData!): ValueSet!
}

type ValueSet {
  id: ID!
  code: String!
  description: String
  name: String!
  codeSet: String!
}

input ValueSetData {
  code: String!
  description: String
  name: String!
  codeSet: String!
  values: [Value]
}

input Value {
 code: String!
 display: String!
 value: String!
 displayOrder: Int! 
 comment: String

}