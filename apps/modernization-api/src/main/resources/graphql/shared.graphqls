scalar Date
scalar DateTime

input Page {
  pageSize: Int!
  pageNumber: Int!
}

input SortablePage {
  pageSize: Int
  pageNumber: Int
  sortField: SortField
  sortFieldText: String
  sortDirection: SortDirection
}

enum SortField {
  lastNm
  birthTime
  relevance
}

enum RecordStatus {
  ACTIVE
  INACTIVE
  LOG_DEL
  SUPERCEDED
}

enum PregnancyStatus {
  YES
  NO
  UNKNOWN
}
