scalar Date
scalar DateTime

input Page {
  pageSize: Int!
  pageNumber: Int!
}

input SortablePage {
  pageSize: Int
  pageNumber: Int
  sortField: SortField
  sortFieldText: String
  sortDirection: SortDirection
}

enum SortField {
  address
  birthTime
  city
  county
  country
  firstNm
  id
  identification
  lastNm
  local_id
  phoneNumber
  relevance
  sex
  state
  zip
}

enum RecordStatus {
  ACTIVE
  INACTIVE
  LOG_DEL
  SUPERCEDED
}

enum PregnancyStatus {
  YES
  NO
  UNKNOWN
}
