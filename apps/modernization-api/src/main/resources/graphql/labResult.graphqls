extend type Query {
    findLocalCodedResults(searchText: String!, page: Page): LocalCodedResults!
}

type LocalCodedResults {
    content: [LabResult]!
    total: Int!
}
type LabResult {
    id: LabResultId
    nbsUid: ID
    labResultDescTxt: String
}

type LabResultId {
    labResultCd: String
    laboratoryId: String
}
