extend type Query {
    findAllMaritalStatus(page: Page): maritalStatusResults!
}

type maritalStatusResults {
    content: [maritalStatus]!
    total: Int!
}

type maritalStatus {
    id: maritalStatusId!
    codeShortDescTxt: String
}

type maritalStatusId {
    codeSetNm: String!
    code: String!
}
