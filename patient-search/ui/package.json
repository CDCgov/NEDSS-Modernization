{
    "name": "ui",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@apollo/client": "^3.6.9",
        "@hookform/resolvers": "^2.9.8",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^12",
        "@testing-library/user-event": "^12",
        "@trussworks/react-uswds": "^3.2.0",
        "@types/jest": "^28.1.6",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "classnames": "^2.3.2",
        "cypress-cucumber-preprocessor": "^4.3.1",
        "graphql": "^16.6.0",
        "lodash.debounce": "^4.0.8",
        "moment": "^2.29.4",
        "multiselect-react-dropdown": "^2.0.25",
        "react": "^17",
        "react-dom": "^17",
        "react-hook-form": "^7.36.1",
        "react-router-dom": "^6.3.0",
        "react-scripts": "5.0.1",
        "typescript": "^4.7.4",
        "web-vitals": "^2.1.4",
        "yup": "^0.32.11"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "eslint .",
        "lint:fix": "eslint --fix",
        "cy:headless": "./node_modules/.bin/cypress run",
        "cy:browser": "./node_modules/.bin/cypress run --headed --no-exit",
        "cy:browser:open": "./node_modules/.bin/cypress open",
        "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
        "generate": "npm run consolidate-graphqls && npm run graphql:gql && npm run graphql:codegen && npm run openapi:codegen",
        "consolidate-graphqls": "rm -f src/generated/schema.graphqls && for f in ../api/src/main/resources/graphql/*.graphqls; do cat $f >> src/generated/schema.graphqls; done",
        "graphql:gql": "npx gqlg --schemaFilePath src/generated/schema.graphqls --destDirPath ./src/generated/gql",
        "graphql:codegen": "export API_TOKEN=$(curl http://localhost:8080/login -X POST --data '{\"username\":\"msa\"}' -H 'Content-Type:Application/JSON' | jq -r '.token') && graphql-codegen --config src/codegen.ts",
        "openapi:codegen": "npx openapi-typescript-codegen --input http://localhost:8080/v2/api-docs --output ./src/generated/ --useOptions && npm run openapi:set-port ",
        "openapi:set-port": "sed -i '' -e \"s/'http:\\/\\/localhost:8080'/Config.modernizationUrl/\" src/generated/core/OpenAPI.ts && echo \"import { Config } from '../../../src/config';\" >> src/generated/core/OpenAPI.ts"
    },
    "proxy": "http://localhost:8080/",
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^2.13.1",
        "@graphql-codegen/fragment-matcher": "^3.3.1",
        "@graphql-codegen/typescript": "^2.7.3",
        "@graphql-codegen/typescript-operations": "^2.5.3",
        "@graphql-codegen/typescript-react-apollo": "^3.3.3",
        "@testing-library/cypress": "^8.0.3",
        "@testing-library/react-hooks": "^7.0.2",
        "@types/lodash.debounce": "^4.0.7",
        "@types/node": "^18.11.5",
        "@typescript-eslint/eslint-plugin": "^5.32.0",
        "@typescript-eslint/parser": "^5.32.0",
        "cypress": "^10.11.0",
        "eslint": "^8.21.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.30.1",
        "gqlg": "1.0.2",
        "openapi-typescript-codegen": "^0.23.0",
        "prettier": "^2.7.1",
        "sass": "^1.54.0",
        "vite": "^3.2.0"
    },
    "cypress-cucumber-preprocessor": {
        "nonGlobalStepDefinitions": false,
        "stepDefinitions": "cypress/support/step_definitions"
    }
}
